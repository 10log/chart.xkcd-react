{
  "name": "chart.xkcd-react",
  "version": "1.0.0",
  "description": "React wrapper for chart.xkcd with TypeScript support",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/10log/chart.xkcd-react.git"
  },
  "author": "10log",
  "license": "MIT",
  "peerDependencies": {
    "chart.xkcd": "^1.1.12",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["esbuild"]
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "chart.xkcd": "^1.1.12",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
